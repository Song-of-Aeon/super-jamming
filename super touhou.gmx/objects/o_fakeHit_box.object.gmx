    if (mouse_x &gt; x and mouse_x &lt; x+width and mouse_y &gt; y and mouse_y &lt; y+height and active){
        activemax = ceil(sprite_get_number(HITBOXMAKER.currentSprite)*HITBOXMAKER.progress)
    }
}
